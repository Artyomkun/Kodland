{
  "name": "corsbridge",
  "version": "1.0.2",
  "description": "Zero-Config CORS Proxy Client for Developers",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "bin": {
    "corsbridge": "dist/cli.mjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "prepublishOnly": "npm run build",
    "test": "vitest"
  },
  "keywords": [
    "cors",
    "proxy",
    "fetch",
    "api",
    "http",
    "cors-bypass",
    "cors-proxy",
    "zero-config"
  ],
  "author": "syrins",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/syrins/corsbridge.git"
  },
  "bugs": {
    "url": "https://github.com/syrins/corsbridge/issues"
  },
  "homepage": "https://cors.syrins.tech",
  "dependencies": {
    "@types/node": "^20.10.0",
    "undici": "^6.19.8"
  },
  "devDependencies": {
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^4.0.14"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
